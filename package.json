{
  "name": "portfolio-wendelin-tech",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "slicemachine": "start-slicemachine",
    "storybook": "start-storybook -p 6006 --static-dir ./public",
    "build-storybook": "build-storybook --static-dir public",
    "tab:next": "ttab -t 'Run Next dev' npm run dev",
