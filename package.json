    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-react-require": "^3.1.3",
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "prettier": "2.5.1",
